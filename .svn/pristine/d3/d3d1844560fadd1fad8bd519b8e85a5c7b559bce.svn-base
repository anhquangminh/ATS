
@{
    ViewBag.Title = "Excel";
    Layout = "~/Views/Shared/_LayoutATS.cshtml";
}
<style type="text/css">
    .table {
        border: 0px;
    }

    td {
        border: 0px;
    }

    table > tbody > tr > td {
        border: 0px;
    }

    table > tfoot > tr > th > select {
        max-width: 80px;
    }

    .table td, .table th {
        vertical-align: middle;
    }

    .content-header {
        padding: 5px .5rem;
    }

    .canvasjs-chart-toolbar {
        display: none;
    }

    .button {
        background-color: #004A7F;
        -webkit-border-radius: 10px;
        border-radius: 10px;
        border: none;
        color: #FFFFFF;
        cursor: pointer;
        display: inline-block;
        font-family: Arial;
        font-size: 20px;
        padding: 5px 10px;
        text-align: center;
        text-decoration: none;
    }

    table thead {
        background-color: #4a5a5482;
    }
    table th, td {
        white-space: nowrap;
    }

</style>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6 col-12">
                    <ol class="breadcrumb float-sm-left">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Tải lên thư mục excel</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                @if (!string.IsNullOrEmpty(ViewBag.Message))
                {
                    <script type="text/javascript">
                           alert("@ViewBag.Message");
                    </script>
                }
                <div class="card col-6">
                    <div class="card-header">
                        <h4>Tải lên thư mục excel</h4>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            @using (Html.BeginForm("ExcelGA", "ATS", FormMethod.Post, new { enctype = "multipart/form-data", @class = "col-12" }))
                            {
                                <div class="form-group">
                                    <div class="col-md-12">
                                        <input name="file" class="form-control" type="file" placeholder="" style="padding:3px;" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-3">
                                        <input type="submit" class="btn-default " value="Tải lên" />
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                    <div class="card-footer">
                        <div> <label class="text-success">@ViewBag.Message</label></div>
                    </div>
                </div>
            </div>
            <div class="row">
                <section class="col-lg-12 connectedSortable">
                    <div class="card">
                        <div class="card-header" style="background-color: #1686ff; color: white;">
                            <h3 class="card-title"> <i class="fas fa-address-book mr-1"></i>Thông tin</h3>
                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                    <i class="fas fa-minus"></i>
                                </button>
                                <button type="button" class="btn btn-tool" data-card-widget="remove">
                                    <i class="fas fa-times"></i>
                                </button>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class=" table-responsive">
                                @if (ViewBag.Data != null)
                                {
                                    <table class="table table-bordered  table-hover" id="table_message" style="width:100%; border-collapse: collapse;font-family: arial; font-size:13px;">
                                        <thead>
                                            <tr>
                                                <th></th>
                                                <th></th>
                                                <th>mathe</th>
                                                <th>hoten</th>
                                                <th>hotentq</th>
                                                <th>khuxuong</th>
                                                <th>bu</th>
                                                <th>cft</th>
                                                <th>toanha</th>
                                                <th>bophan</th>
                                                <th>ngayvaoxuong</th>
                                                <th>phancap</th>
                                                <th>capbac</th>
                                                <th>gioitinh</th>
                                                <th>machiphi</th>
                                                <th>ngaysinh</th>
                                                <th>cmt</th>
                                                <th>ngayvaoKTX</th>
                                                <th>ngayraKTX</th>
                                                <th>quequan</th>
                                                <th>toaKTX</th>
                                                <th>sophong</th>
                                                <th>sogiuong</th>
                                                <th>ghichu</th>
                                                <th>nguoitailen</th>
                                                <th>thoigiantai</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in ViewBag.Data)
                                            {
                                                <tr>
                                                    <td style="text-align:center"><a href="#" onclick="return GetbyID('@item.id')"><i class="fas fa-edit"></i></a></td>
                                                    <td style="text-align:center"><a href="#" onclick="DeleteGA('@item.id')"><i class="far fa-trash-alt"></i></a></td>
                                                    <td>@item.mathe</td>
                                                    <td>@item.hoten</td>
                                                    <td>@item.hotentq</td>
                                                    <td>@item.khuxuong</td>
                                                    <td>@item.bu</td>
                                                    <td>@item.cft</td>
                                                    <td>@item.toanha</td>
                                                    <td>@item.bophan</td>
                                                    <td>@item.ngayvaoxuong</td>
                                                    <td>@item.phancap</td>
                                                    <td>@item.capbac</td>
                                                    <td>@item.gioitinh</td>
                                                    <td>@item.machiphi</td>
                                                    <td>@item.ngaysinh</td>
                                                    <td>@item.cmt</td>
                                                    <td>@item.ngayvaoKTX</td>
                                                    <td>@item.ngayraKTX</td>
                                                    <td>@item.quequan</td>
                                                    <td>@item.toaKTX</td>
                                                    <td>@item.sophong</td>
                                                    <td>@item.sogiuong</td>
                                                    <td>@item.ghichu</td>
                                                    <td>@item.nguoitailen</td>
                                                    <td>@item.thoigiantai</td>
                                                </tr>
                                            }
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th></th>
                                                <th></th>
                                                <th>mathe</th>
                                                <th>hoten</th>
                                                <th>hotentq</th>
                                                <th>khuxuong</th>
                                                <th>bu</th>
                                                <th>cft</th>
                                                <th>toanha</th>
                                                <th>bophan</th>
                                                <th>ngayvaoxuong</th>
                                                <th>phancap</th>
                                                <th>capbac</th>
                                                <th>gioitinh</th>
                                                <th>machiphi</th>
                                                <th>ngaysinh</th>
                                                <th>cmt</th>
                                                <th>ngayvaoKTX</th>
                                                <th>ngayraKTX</th>
                                                <th>quequan</th>
                                                <th>toaKTX</th>
                                                <th>sophong</th>
                                                <th>sogiuong</th>
                                                <th>ghichu</th>
                                                <th>nguoitailen</th>
                                                <th>thoigiantai</th>
                                            </tr>
                                        </tfoot>
                                    </table>

                                }
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <a id="back-to-top" href="#" class="btn btn-primary back-to-top" role="button" aria-label="Scroll to top">
                <i class="fas fa-chevron-up"></i>
            </a>
        </div>
    </section>
    @*View edit*@
